{
  "name": "talio-backend",
  "version": "0.0.1",
  "private": true,
  "scripts": {
    "start": "pm2-runtime build/src",
    "dev": "ts-node-dev -C ttypescript --respawn --transpile-only --ignore-watch node_modules -r tsconfig-paths/register src",
    "prebuild": "rimraf build",
    "build": "ttsc --noEmit false --diagnostics",
    "apidoc": "apidoc -i src/Routes -o static/apidoc",
    "emails:dev": "gulp",
    "emails:build": "gulp build --production",
    "execute:file": "yarn ts-node -C ttypescript -r tsconfig-paths/register",
    "db:dump": "yarn execute:file db/createDump",
    "db:migrate": "sequelize db:migrate",
    "db:rollback": "sequelize db:migrate:undo",
    "db:rollback:all": "sequelize db:migrate:undo:all",
    "db:migration": "sequelize migration:generate",
    "db:seed:all": "npx sequelize-cli db:seed:all",
    "db:seed:undo": "sequelize db:seed:undo",
    "db:seed:create": "sequelize seed:generate",
    "typecheck": "ttsc -p .",
    "test": "yarn jest --maxWorkers=1 --forceExit --logHeapUsage",
    "test:watch": "yarn test --watch",
    "test:coverage": "yarn test --coverage",
    "test:migrate": "NODE_ENV=test yarn db:migrate",
    "lint": "CI=true eslint src/**/*.ts",
    "lint:fix": "yarn lint --fix",
    "postinstall": "husky install",
    "create-tsseed": "ts-node -r tsconfig-paths/register src/Services/TsSeeds/index.ts --action=create",
    "run-tsseed": "ts-node -r tsconfig-paths/register src/Services/TsSeeds/index.ts --action=seed"
  },
  "dependencies": {
    "@aclify/aclify": "2.2.0",
    "@aws-sdk/client-s3": "3.10.0",
    "@aws-sdk/s3-request-presigner": "3.10.0",
    "@koa/cors": "3.1.0",
    "@koa/router": "10.0.0",
    "@polkadot/api": "^7.15.1",
    "@polkadot/util-crypto": "8.7.1",
    "@polkadot/wasm-crypto": "^5.1.1",
    "@sendgrid/mail": "7.4.2",
    "@sentry/node": "6.2.4",
    "@startupcraft/dotenv-config": "0.0.2",
    "apollo-server-koa": "2.22.2",
    "aws-sdk": "2.891.0",
    "bcryptjs": "2.4.3",
    "bull-board": "1.2.0",
    "bullmq": "1.15.0",
    "chalk": "4.1.0",
    "class-validator": "0.13.1",
    "dataloader": "2.0.0",
    "ejs": "3.1.8",
    "express": "4.17.1",
    "graphql": "15.5.0",
    "graphql-redis-subscriptions": "2.3.1",
    "graphql-type-json": "0.3.2",
    "graphql-ws": "4.2.2",
    "http-shutdown": "1.2.2",
    "ioredis": "4.24.2",
    "jsonwebtoken": "8.5.1",
    "koa": "2.13.1",
    "koa-body": "4.2.0",
    "koa-compress": "5.0.1",
    "koa-passport": "4.1.4",
    "koa-static": "5.0.0",
    "lodash": "4.17.21",
    "luxon": "1.26.0",
    "ms": "2.1.3",
    "nanoid": "3.1.22",
    "node-fetch": "3.2.2",
