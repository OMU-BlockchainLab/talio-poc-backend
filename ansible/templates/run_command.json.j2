{
  "cluster": "{{ cluster }}",
  "taskDefinition": "{{ task_definition_arn }}",
  "launchType": "FARGATE",
  "overrides": {
    "containerOverrides": [
      {
        "name": "backend",
        "command": {{ command }}
      }
    ]
  },
  "networkConfiguration": {
    "awsvpcConfiguration": {
      "subnets": {{ subnets | to_json }},
      "securityGroups": {{ security_groups | to_json }},
      "assignPublicIp": "ENABLED"
    }
  }
}
